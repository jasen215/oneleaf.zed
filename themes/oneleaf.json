{
  "$schema": "https://zed.dev/schema/themes/v0.2.0.json",
  "name": "One Leaf",
  "author": "Jasen <jasen215@gmail.com>",
  "themes": [
    {
      "name": "One Leaf",
      "appearance": "light",
      "style": {
        "background": "#FFFFFF",
        "text": "#424242",
        "border": "#E0E0E0",
        "line_highlight": "#EEF1F5",
        "selection": {
          "background": "#A7CAFF"
        },
        "line_number": "#8c8fa1",
        "active_line_number": "#424242",
        "surface.background": "#e6e9ef",
        "text.muted": "#5c5f77",
        "text.placeholder": "#acb0be",
        "text.disabled": "#bcc0cc",
        "icon": "#4c4f69",
        "icon.muted": "#8c8fa1",
        "icon.disabled": "#9ca0b0",
        "icon.placeholder": "#acb0be",
        "search.match_background": "#c0c0c0",
        "panel.background": "#ebedef",
        "players": [
          { "cursor": "#424242", "selection": "#A7CAFF" },
          { "cursor": "#881181", "selection": "#05336940" }
        ],
        "tab_bar.background": "#dadada",
        "tab.inactive_background": "#d0d0d0",
        "tab.active_background": "#eff1f5",
        "terminal.background": "#FFFFFF",
        "terminal.foreground": "#424242",
        "version_control.added": "#027101",
        "version_control.deleted": "#d20f39",
        "version_control.modified": "#895503",
        "version_control.renamed": "#209fb5",
        "version_control.conflict": "#fe640b",
        "version_control.ignored": "#9ca0b0",
        "link_text.hover": "#00000",
        "conflict": "#00000",
        "created": "#00000",
        "deleted": "#00000",
        "hidden": "#ff000",
        "hint": "#00000",
        "ignored": "#00000",
        "modified": "#895503",
        "predictive": "#00000",
        "renamed": "#00000",
        "info": "#00000",
        "warning": "#895503",
        "error": "#00000",
        "success": "#00000",
        "unreachable": "#00000",
        "syntax": {
          "comment": {
            "color": "#009000",
            "font_style": "italic"
          },
          "string": { "color": "#ED7722" },
          "keyword": { "color": "#CA0063" },
          "keyword.control": { "color": "#AA2063" },
          "keyword.operator": { "color": "#000000" },
          "storage.type": { "color": "#CA0063" },
          "function": { "color": "#053369" },
          "function.call": { "color": "#163369" },
          "variable": { "color": "#916305" },
          "variable.parameter": { "color": "#053369" },
          "variable.other": { "color": "#996633" },
          "type": { "color": "#CC4C07" },
          "type.class": { "color": "#CC4C07" },
          "type.inherited": { "color": "#CC4C07" },
          "constant": { "color": "#916319" },
          "constant.numeric": { "color": "#0F20F6" },
          "constant.language": { "color": "#000000" },
          "attribute": { "color": "#881181" },
          "property": { "color": "#881181" },
          "error": { "color": "#DA2000" },
          "warning": { "color": "#EDBA00" },
          "number": { "color": "#0F20F6" },
          "boolean": { "color": "#916319" },
          "tag": { "color": "#881181" },
          "operator": { "color": "#000000" },
          "punctuation": { "color": "#000000" },
          "punctuation.bracket": { "color": "#F02A1D" },
          "punctuation.delimiter": { "color": "#000000" },
          "link_text": { "color": "#053369" },
          "link_uri": { "color": "#ED7722" }
        }
      }
    }
  ]
}
